*insert basic labjack startup here*
while true
tic;
  while toc <= 45;
    read crosswalk switch for north/south
  end
if (north/south crosswalk == 1)
tic;
  while toc <= 45;
    turn on north/south crosswalk light
  end
end
if (east/west crosswalk == 1)
tic;
  while toc <= 45;
    turn on east/west crosswalk light
  end
end
